<beans>
	<bean name="catBody" class="summerframework.test.CatBody">
		<property name="color" value="yellow" />
	</bean>
	
	<bean id="cat" class="summerframework.test.Cat">
		<property name="nickName" value="orange" />
		<property name="legs" value="4" />
		<property name="body" ref="catBody" />
	</bean>
</beans>